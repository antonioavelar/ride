{
  "compilerOptions": {
    "declaration": true,
    "outDir": "./dist",
    "module": "commonjs",
    "allowJs": true,
    "noEmit": false,
    "composite": true,
    "baseUrl": "./src/",
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
  },
  "include": [
    "./src/**/*",
    "./config/**/*.json"
  ],
}
